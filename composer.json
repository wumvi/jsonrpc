{
  "name": "wumvi/jsonrpc",
  "description": "Base http framework",
  "type": "library",
  "autoload": {
    "psr-4": {
      "Wumvi\\JsonRpc\\": "src"
    }
  },
  "require": {
    "php": "~7.1",
    "symfony/dependency-injection": "@stable",
    "symfony/config": "^4.2",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "@stable",
    "squizlabs/php_codesniffer": "@stable",
    "phan/phan": "@stable",
    "phpstan/phpstan": "@stable",
    "overtrue/phplint": "@stable"
  },
  "config": {
    "secure-http": false
  },
  "authors": [
    {
      "name": "Kozlenko Vitaliy",
      "role": "Developer"
    }
  ],
  "scripts": {
    "phpstan": [
      "vendor/bin/phpstan analyze --level=7 src -c phpstan.neon"
    ],
    "phpunit": [
      "vendor/bin/phpunit"
    ],
    "phplint": [
      "vendor/bin/phplint"
    ],
    "phpcs": [
      "vendor/bin/phpcs src --parallel=10 --runtime-set php_version 71000 -p --standard=ruleset.xml"
    ],
    "test": [
      "@phpstan",
      "@phpunit",
      "@phplint",
      "@phpcs"
    ]
  },
  "license": "MIT"
}
